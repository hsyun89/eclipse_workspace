	<h1>자바스크립트의 객체 생성과 사용(생성자함수)</h1>
	<hr>
	<script src="util.js"></script>
	<script>
	function Student(p1,p2,p3,p4){
				this.name = p1,
				this.kor = p2,
				this.eng = p3,
				this.math = p4,
			getSum : function() {
				return this.kor + this.eng + this.math;
			},
			getAvg : function() {
				return this.getSum() / 3;
			},
			toString : function() {
				return this.name + "학생의 총점은 " + this.getSum() + "입니다.";
			}
		}
		

		
/* 		name : '또치',
		kor : 80,
		eng : 90,
		math : 80,
		
		name : '도우너',
		kor : 85,
		eng : 70,
		math : 80, */
		
