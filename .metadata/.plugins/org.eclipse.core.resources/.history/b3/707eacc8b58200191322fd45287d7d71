<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<script>
		function calc() {
			if (arguments.length == 0) {
				result = 0;
			} else {
				var sum = 0;
				for (var i = 0; i < arguments.length; i++) {
					sum += parseInt(arguments[i]);
				}
				if (isNaN(sum))
					result = "숫자만 전달하세요";
				else
					result = sum;
			}
			return result
		}
		var data = [ calc(), calc(10, 20, '30'), calc(10, '가나다', 20),
				calc(1, 2, 3, 4, 5) ]
		for(var i in data){
			document.write("<h3>" + data[i] + "</h3><br>");
		}

		document.write("<h3>" + calc() + "</h3><br>");
		document.write("<h3>" + calc(10, 20, '30') + "</h3><br>");
		document.write("<h3>" + calc(10, '가나다', 20) + "</h3><br>");
		document.write("<h3>" + calc(1, 2, 3, 4, 5) + "</h3><br>");
	</script>
</body>
</html>